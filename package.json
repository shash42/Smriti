  "dependencies": {
    "algorithmia": "^0.3.10",
    "body-parser": "^1.19.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "natural": "^0.6.3",
    "path": "^0.12.7",
    "pos": "^0.4.2"
  },
  "devDependencies": {
